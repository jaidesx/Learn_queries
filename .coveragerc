# .coveragerc
[run]
omit =
    */migrations/*
    */settings/*
    */admin.py
    */wsgi.py
    */asgi.py
    */manage.py

[report]
exclude_lines =
    pragma: no cover
    def __str__
    if self\.debug
    if settings\.DEBUG
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:

[html]
directory = htmlcov
